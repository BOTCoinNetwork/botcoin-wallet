{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "/Users/danu/code/src/github.com/mosaicnetworks/evml-client/**/*.*"
    ]
  },
  "rules": {
    "no-console": false,
    "no-shadowed-variable": false,
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false
  }
}
